#: ../../faq.rst:113
msgid "`Commcare HQ <http://commcarehq.com/>`_;"
msgstr ""

#: ../../faq.rst:114
msgid "`DoForms <http://doforms.com/>`_;"
msgstr ""

#: ../../faq.rst:115
msgid "`DataWinners <http://datawinners.com/>`_;"
msgstr ""

#: ../../faq.rst:116
msgid "`ViewWorld <http://viewworld.dk/>`_;"
msgstr ""

#: ../../faq.rst:117
msgid "`PhiCollect <http://webfirst.com/phicollect>`_;"
msgstr ""

#: ../../faq.rst:119
msgid "While other are ODK compatible:"
msgstr ""

#: ../../faq.rst:121
msgid "`JavaRosa <http://www.dimagi.com/javarosa/>`_;"
msgstr ""

#: ../../faq.rst:122
msgid "`OpenXData <http://www.openxdata.org/>`_;"
